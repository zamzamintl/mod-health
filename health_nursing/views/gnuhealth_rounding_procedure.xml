<odoo>
    <record id="gnuhealth_procedure_form_view" model="ir.ui.view">
        <field name="name">gnuhealth.procedure.form.view</field>
        <field name="model">gnuhealth.procedure</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="Procedure" modifiers="{}">
                <group col="4">
                    <field name="name"/>
                    <newline/>
                    <separator string="Procedure Details / Incidents" colspan="4" id="separator_procedure_details"/>
                    <newline/>
                    <field name="notes" colspan="4"/>
                </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>