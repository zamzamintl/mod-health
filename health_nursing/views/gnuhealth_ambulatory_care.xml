<odoo>
    <record id="gnuhealth_ambulatory_procedure_form_view" model="ir.ui.view">
        <field name="name">gnuhealth.ambulatory.procedure.form.view</field>
        <field name="model">gnuhealth.ambulatory.care.procedure</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="Ambulatory Care Procedure" modifiers="{}">
                    <group col="4">
                        <field name="procedure"/>
                        <newline/>
                        <separator string="Procedure Details / Incidents" colspan="4" id="separator_procedure_details"/>
                        <newline/>
                        <field name="comments" colspan="4"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
