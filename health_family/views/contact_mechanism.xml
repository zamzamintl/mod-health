<odoo>
	<record model="ir.ui.view" id="contact_mechanism_tree">
		<field name="name">contact.mechanism.tree</field>
		<field name="model">medical.family</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<xpath expr="/tree/field[@name=&quot;type&quot;]" position="after">
				<field name="emergency"/>
				<field name="remarks"/>
			</xpath>
		</field>
	</record>
</odoo>
