<odoo>
    <data>
        <record id='gnuhealth_icu_ventilation_view_tree' model='ir.ui.view'>
            <field name="name">ICU Ventilation</field>
            <field name="model">icu.ventilation</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="current_mv"/>
                    <field name="ventilation"/>
                    <field name="mv_start" />
                    <field name="mv_end" />
                    <field name="mv_period"/>
                    <field name="remarks" />
                </tree>
            </field>
        </record>
    </data>
</odoo>
