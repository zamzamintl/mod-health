<odoo>
    <record model="ir.ui.view" id="medical_procedure_form">
        <field name="name">medical.procedure.form</field>
        <field name="model">medical.procedure</field>
        <field name="arch" type="xml">
            <form>
                <label string ="." for="name"/>
                <field name="name"/>
                <label string ="." for="description"/>
                <field name="description"/>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="medical_procedure_tree">
        <field name="name">medical.procedure.tree</field>
        <field name="model">medical.procedure</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>
</odoo>
