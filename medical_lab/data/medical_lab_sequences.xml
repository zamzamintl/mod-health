<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for lab test report -->
        <record id="seq_medical_lab_test" model="ir.sequence">
            <field name="name">Lab Test</field>
            <field name="code">medical.lab</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>

        <!-- Sequences for lab request -->
        <record id="seq_medical_lab_request" model="ir.sequence">
            <field name="name">Lab Request</field>
            <field name="code">medical.patient.lab.test</field>
            <field name="prefix">LRQ</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>