<odoo>
    <data>
        <!-- 'imaging.test.type' -->
        <record id="imaging_test_type_action_all" model="ir.actions.act_window">
            <field name="name">Imaging Test Type Action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">imaging.test.type</field>
            <field name="view_mode">kanban,form,tree</field>
        </record>
        <record id='imaging_test_type_view_form' model='ir.ui.view'>
            <field name="name">Imaging Test Type Form</field>
            <field name="model">imaging.test.type</field>
            <field name="arch" type="xml">
                <form string="Imaging Test Type">
                    <sheet>
                        <group string="Imaging Test Type" col="4">
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
